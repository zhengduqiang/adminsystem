<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.adminsystem.mapper.AdminMapper">
    <!--登录-->
    <select id="findAdminLogin" resultType="Admin"
            parameterType="Admin">
        SELECT * FROM admin WHERE admin =#{admin} AND pwd = #{pwd}
    </select>


